    "*.gen.dart": true,
    "**/*.gen.dart": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    "**/.dart_tool": true,
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "*.g.dart": true,
    "**/*.g.dart": true,
    "*.freezed.dart": true,
    "**/*.freezed.dart": true,
    "*.gen.dart": true,
    "**/*.gen.dart": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    "**/.dart_tool": true,
    "**/.fvm": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.g.dart": true,
    "*.freezed.dart": true,
    "**/*.freezed.dart": true,
    "*.gen.dart": true,
    "**/*.gen.dart": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
