{
  "dart.lineLength": 80,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.rulers": [
      80
    ]
  },
  "cSpell.words": [
    "hmac",
    "svgs"
  ],
  "dart.flutterSdkPath": ".fvm/versions/3.32.3",
  "search.exclude": {
    "*.g.dart": true,
    "**/*.g.dart": true,
    "*.freezed.dart": true,
